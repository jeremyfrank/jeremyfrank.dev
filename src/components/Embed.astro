---
type Props = {
  url: string
  title?: string
  classes?: string
  width?: number
  height?: number
}

const { url, title, classes, width = 800, height = 400 } = Astro.props
---

<iframe
  src={url}
  title={title}
  width={width}
  height={height}
  class:list={['rounded-md w-full', classes]}
  loading="lazy"></iframe>
